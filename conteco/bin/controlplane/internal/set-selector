#!/usr/bin/env bash

selector="$1"
if [[ "$selector" == "." ]] ; then
	selector=
fi
if [[ $selector != "${CONTECO_REALM_RUNTIME}/${CONTECO_CONTROLPLANE}."* && $selector != "${CONTECO_REALM_RUNTIME}/${CONTECO_CONTROLPLANE}" ]] ; then
	if [[ $selector == "${CONTECO_CONTROLPLANE}."* ]] ; then
		selector="${CONTECO_REALM_RUNTIME}/${selector}"
	else
		selector="${CONTECO_REALM_RUNTIME}/${CONTECO_CONTROLPLANE}.${selector}"
	fi
fi
echo $selector
